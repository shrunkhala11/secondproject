
.container{
	height:100vh;
	display:grid;
	grid-gap:1px;
	border:1px solid black;
	grid-template-areas:"header header header header" "main main main main"
 "maincontent maincontent maincontent maincontent"  "section section section section"
 "aside aside aside aside" ;
}
.header{
	height:100px;
	grid-area:header;
	background-color:rgb(234, 229, 229);
	border:1px solid black;
}
.header1{
	height:100px;
	width:20%;
	float:left;
	border:1px solid black;
}
.list{
	height:70px;
	width:50px;
	margin:15px;
    border:1px solid orange;
}
.header2{
	height:100px;
	width:49%;
	float:left;
	border:1px solid black;
}
.header3{
	height:100px;
	width:29%;
	float:left;
	border:1px solid black;
}
.main{
	height:600px;
	grid-area:main;
	
	background-color:rgb(234, 229, 229);
	border:1px solid black;
}
.main1{
	width:100%;
	height:300px;
	border:1px solid black;
}
.main2{
	height:300px;
	width:100%;
	border:1px solid black;
}

.maincontent{
	height:900px;
	grid-area:maincontent;
	
	background-color:rgb(234, 229, 229);
	border:1px solid black;
}
.box{
	height:250px;
	width:80%;
	margin:40px;
	background-color:white;
	border:1px solid black;
}
.section{
	height:2650px;
	grid-area:section;
	
	background-color:rgb(234, 229, 229);
	border:1px solid black;
}
.sechead{
	height:100px;
	width:100%;
	background-color:white;
	border:1px solid black;
}

.section1{
	height:250px;
	width:80%;
	margin-left:40px;
	margin-right:40px;
	margin-top:60px;
	margin-bottom:60px;
    background-color:white;
	border:1px solid black;
}
.aside{
	height:400px;
	grid-area:aside;
	border:1px solid black;
}
.article{
    height:800px;
	grid-area:article;
	 background-color:white;
	border:1px solid black;	
}
.art{
	height:170px;
	width:80%;
	margin:30px;
	background-color:pink;
	border:1px solid black;
}